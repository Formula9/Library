{
    "name"             : "formula9/library",
    "description"      : "A library of classes and helpers to support formula9.",
    "type"             : "library",
    "license"          : "MIT",
    "authors"          : [
        {
            "name" : "Greg Truesdell",
            "email": "odd.greg@gmail.com"
        }
    ],
    "repositories"     : [
        { "type": "vcs", "url": "https://github.com/Formula9/Collections.git" }
    ],
    "autoload"         : {
        "classmap": [
            "src",
            "tests"
        ],
        "psr-4"   : {
            "Nine\\Library\\": "src"
        }
    },
    "require"          : {
        "formula9/collections": "0.4.2.x-dev"
    },
    "require-dev"      : {
        "phpunit/phpunit"          : "^5.1@dev",
        "tracy/tracy"              : "^2.4@dev",
        "phpunit/php-code-coverage": "^4"
    },
    "minimum-stability": "dev"
}
